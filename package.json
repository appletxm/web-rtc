{
  "name": "samples-server",
  "version": "1.1.0",
  "description": "Front end and menu for the MDN Web Docs sample server; used for special examples requiring back-end support",
  "main": "scripts/server.js",
  "homepage": "http://mdn-samples.mozilla.org/",
  "scripts": {
    "clean": "rimraf build/*",
    "server": "node scripts/server.js",
    "ws-server": "node scripts/chatserver.js",
    "uglify": "uglifyjs build/index.js -o build/index.min.js -c -m",
    "rollup:watch": "rollup --config config/rollup.config.dev.js --watch",
    "rollup:build": "rollup --config config/rollup.config.prod.js && npm run uglify",
    "start": "npm run clean && sudo concurrently --kill-others \"npm run server\" \"npm run ws-server\" \"npm run rollup:watch\"",
    "debug": "sudo npm run clean && node scripts/server.js && node --inspect scripts/chatserver.js && npm run rollup:watch",
    "postinstall": "cp node_modules/webrtc-adapter/out/adapter.js scripts/"
  },
  "keywords": [
    "mdn",
    "mozilla",
    "samples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/web-rtc.git"
  },
  "author": "Eric Shepherd / Mozilla",
  "bugs": "https://github.com/issues",
  "license": "CC0-1.0",
  "dependencies": {
    "escape-html": "^1.0.3",
    "express": "^4.16.4",
    "ip": "^1.1.5",
    "webrtc-adapter": "^1.4.0",
    "websocket": "^1.0.29"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime-corejs3": "^7.10.2",
    "babel-eslint": "8.0.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "eslint": "4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "event-source-polyfill": "0.0.12",
    "hex64": "^0.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^6.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "uglifyjs": "^2.4.11"
  }
}
